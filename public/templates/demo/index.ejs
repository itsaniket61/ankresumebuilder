<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css">
    <style>
        html {
            -webkit-print-color-adjust: exact;
        }

        .hero {
            background-color: #1B4472;
        }

        .section-title {
            color: #128CCD;
            border-bottom: solid 3px #128CCD;
            margin-top: 10px;
            text-transform: uppercase;
            font-weight: 700;
        }
    </style>
</head>

<body>
    <div class="">
        <div class="text-light hero py-4 px-2">
            <h1>
                <%=data.name%>
            </h1>
            <h4>
                <%=data.jobRole%>
            </h4>
        </div>
        <div class="p-2">
            <h4 class="section-title">Introduction</h2>
            <p>
                <%=data.introduction%>
            </p>
        </div>
        <!--Row Starts Here-->
        <div class="row p-2">
            <div class="col">
                <section>
                    <h4 class="section-title">Skills</h2>
                    <p>
                    <ul>
                        <% for(let i=0; i < data.skills.length; i++) { %>
                            <li><b>
                                    <%=data.skills[i]['title']%>
                                </b>: <%=data.skills[i]['description']%>
                            </li>
                            <% } %>
                    </ul>
                    </p>
                </section>
            </div>
            <div class="col">
                <section>
                    <h4 class="section-title">Education</h2>
                    <p>
                    <ul>
                        <% for(let i=0; i < data.education.length; i++) { %>
                            <li><b>
                                    <%=data.education[i]['title']%>
                                </b>: <%=data.education[i]['description']%>
                            </li>
                            <% } %>
                    </ul>
                    </p>
                </section>
            </div>
        </div>
        <!--Row Ends Here-->
        <!--Row Starts Here-->
        <div class="row p-2">
            <div class="col">
                <section>
                    <h4 class="section-title">Contact</h2>
                    <p>
                    <ul>
                        <% for(let i=0; i < data.contacts.length; i++) { %>
                            <li><b>
                                    <%=data.contacts[i]['title']%>
                                </b></li>
                            <% } %>
                    </ul>
                    </p>
                </section>
            </div>
            <div class="col">
                <section>
                    <h4 class="section-title">Experience</h2>
                    <p>
                    <ul>
                        <% for(let i=0; i < data.experience.length; i++) { %>
                            <li><b>
                                    <%=data.experience[i]['title']%>
                                </b>: <%=data.experience[i]['description']%>
                            </li>
                            <% } %>
                    </ul>
                    </p>
                </section>
            </div>
        </div>
        <!--Row Ends Here-->
        <!--Row Starts Here-->
        <div class="row p-2">
            <div class="col">
                <section>
                    <h4 class="section-title">Certifications</h2>
                    <p>
                    <ul>
                        <% for(let i=0; i < data.certifications.length; i++) { %>
                            <li><b>
                                    <%=data.certifications[i]['title']%>
                                </b>: <%=data.experience[i]['description']%>
                            </li>
                            <% } %>
                    </ul>
                    </p>
                </section>
            </div>
            <div class="col">
                <section>
                    <h4 class="section-title">Projects</h2>
                    <p>
                    <ul>
                        <% for(let i=0; i < data.projects.length; i++) { %>
                            <li><b>
                                    <%=data.projects[i]['title']%>
                                </b>: <%=data.projects[i]['description']%>
                            </li>
                            <% } %>
                    </ul>
                    </p>
                </section>
            </div>
        </div>
        <!--Row Ends Here-->
    </div>
</body>

</html>